{
  "_comments": [
    "Valid terminals include: 'Terminal.app' or 'iTerm'",
    "In the editor value change 'default' to 'nano', 'vi', or another terminal based editor.",
    "Hosts will also be read from your ~/.ssh/config or /etc/ssh_config file, if available",
    "For more information on how to configure, please see http://fitztrev.github.io/shuttle/",
    "echo 'â€”->WARNING! Running commands<-- Are you sure? y/n'; read sure; if [ $sure == y ]; then echo running command && ps aux | grep [a]pple; else echo save to history and show... && history -s 'ps aux | grep [a]pple' && osascript -e 'tell application \"System Events\" to keystroke \"p\" using {control down}'; fi"
  ],
  "editor": "default",
  "launch_at_login": true,
  "terminal": "iTerm",
  "iTerm_version": "nightly",
  "default_theme": "Homebrew",
  "open_in": "new",  
  "show_ssh_config_hosts": true,
  "ssh_config_ignore_hosts": [  ],
  "ssh_config_ignore_keywords": [  ],
  "hosts": [
    {
      "Home Servers": [
      	{
          "cmd": "ssh vasili@Ares",
          "inTerminal": "new",
          "name": "Ares",
          "theme": "basic",
          "title": "Ares SSH"
        },
        {
          "cmd": "ssh vasili@Zeus",
          "inTerminal": "new",
          "name": "Zeus",
          "theme": "basic",
          "title": "Zeus SSH"
        },
        {
          "cmd": "ssh vasili@Apollo -p 20",
          "inTerminal": "new",
          "name": "Apollo",
          "theme": "basic",
          "title": "Apollo SSH"
        },
        { 
          "Shutdown Servers": [
            {
              "cmd": "~/Code/Bash/zeus_shutdown",
              "inTerminal": "new",
              "name": "Shutdown Zeus",
              "theme": "basic",
              "title": "Shutdown Zeus"
            }
          ]
        }
      ]   
    },
    {
      "Vagrant": [
        {
          "cmd": "ssh vagrant@127.0.0.1 -p 2222 -i ~/.vagrant.d/insecure_private_key",
          "name": "Vagrant - Opens in default-window-theme-title"
        }
      ]
    }
  ]
}

        